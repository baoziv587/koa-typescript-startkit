    "gulp-filter": "^4.0.0",
    "gulp-mocha": "^2.2.0",
    "gulp-tsb": "^1.10.4",
    "gulp-watch": "^4.3.5",
    "istanbul": "^0.4.3",
    "mocha": "^2.4.5",
    "pm2": "^1.1.3",
    "retyped-request-tsd-ambient": "0.0.0-0",
    "typescript": "^1.8.10"
  }
