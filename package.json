{
  "name": "koa-ts-startkit",
  "version": "0.0.1",
  "description": "koa-ts-startkit",
  "main": "server.js",
  "scripts": {
    "doc:gen": "./node_modules/.bin/apidoc -i src/app/controllers -o ./build/out/public/docs  ",
    "dev": "npm run compile&&./node_modules/.bin/pm2-dev build/out/bootstrap.js --name node-dev",
    "start": "npm run compile&&./node_modules/.bin/pm2 start build/out/bootstrap.js ",
    "stop": "./node_modules/.bin/pm2 stop ",
    "reboot": "./node_modules/.bin/pm2 restart ",
    "compile": "./node_modules/gulp/bin/gulp.js",
    "compile-watch": "./node_modules/gulp/bin/gulp.js watch"
  },
  "repository": {},
  "author": "gaoxiang",
  "license": "ISC",
  "dependencies": {
    "apidoc": "^0.13.2",
    "config": "^1.20.2",
    "iconv-lite": "^0.4.13",
    "js-yaml": "^3.4.2",
    "koa": "^2.0.0",
    "koa-bodyparser": "^2.2.0",
    "koa-compress": "^1.0.9",
    "koa-convert": "^1.2.0",
    "koa-csrf": "^3.0.4",
    "koa-generic-session": "^1.11.3",
    "koa-helmet": "^2.0.0",
    "koa-json": "^1.1.3",
    "koa-logger": "^1.3.0",
    "koa-logs-full": "0.0.8",
    "koa-morgan": "^1.0.1",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "lodash": "^4.13.0",
    "request": "^2.72.0",
    "sequelize": "^3.23.2"
  },
  "devDependencies": {
    "debounce": "^1.0.0",
